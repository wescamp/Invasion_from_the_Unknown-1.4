#, fuzzy
msgid "Strength: melee damage +1, hitpoints +5 <Max XP +20%>"
msgstr "Forza: danni ravvicinati +2, punti ferita +5 <PE max +15%>"

#: Invasion_from_the_Unknown/utils/amla.cfg:42
#, fuzzy
msgid "Strength: melee strikes +1 <Max XP +30%>"
msgstr "Forza: danni ravvicinati +1, <PE max +50%>"

#: Invasion_from_the_Unknown/utils/amla.cfg:58
msgid "Strength: melee damage +1, <Max XP +40%>"
msgstr "Forza: danni ravvicinati +1, <PE max +40%>"

#: Invasion_from_the_Unknown/utils/amla.cfg:74
msgid "Strength: melee strikes +1, <Max XP +50%>"
msgstr "Forza: danni ravvicinati +1, <PE max +50%>"

#: Invasion_from_the_Unknown/utils/amla.cfg:89
msgid "New attack: thorns (pierce 9-3, magical) <Max XP +15%>"
msgstr "Nuovo attacco: spine (perforazione 9-3, magico) <PE max +15%>"

#: Invasion_from_the_Unknown/utils/boss-utils.cfg:18
