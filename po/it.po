#: Invasion_from_the_Unknown/scenarios/23B_Welcome_to_the_Lair/enemies.cfg:40
#, fuzzy
msgid "udesc^Chimera"
msgstr "Limzer"

#. side: type=Chaos Magus
#: Invasion_from_the_Unknown/scenarios/23B_Welcome_to_the_Lair/enemies.cfg:61
